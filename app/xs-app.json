{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/user-api(.*)",
      "target": "$1",
      "service": "sap-approuter-userapi"
    },
    {
      "source": "/service/(.*)$",
      "target": "/service/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },
    {
      "source": "/v2/service/(.*)$",
      "target": "/v2/service/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },
    {
      "source": "^/(.*)$",
      "target": "$1",
      "localDir": "./"
    }
  ]
}
